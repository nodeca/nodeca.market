- self.add_raw_data('search', self.search)
- self.add_raw_data('settings', self.settings)
- self.add_raw_data('pagination', self.pagination)

article#content.market-search-wish
  -
    var navbar_base = {
      level_up: self.link_to('market.index.wish'),
      progress: {
        link_top:    self.link_to('market.search.wish', { $query: self.search }),
        link_bottom: '',
        current:     0,
        total:       0
      }
    };

    const navbar_primary = {
      search_method: 'market.search.wish'
    };

    const navbar_alt = {
      title:    self.t('title'),
      extra_buttons: 'market.search.wish.blocks.page_actions.dropdown'
    };

  != self.partial('@common.blocks.navbar.levelup_progress_apps', { ...navbar_base, ...navbar_primary })
  != self.partial('@common.blocks.navbar.alt_levelup_progress_title', { ...navbar_base, ...navbar_alt })

  nav.breadcrumbs-container
    .layout__container.container-fluid
      != self.partial('@common.blocks.breadcrumbs')

  .layout__container.container-fluid
    != self.partial('@users.blocks.announces')
    != self.partial('@market.blocks.drafts')

    header.page-head.market-search-wish__head
      h1.market-search-wish__title.page-head__title(itemprop='name')= self.t('title')

      != self.partial('@market.search.wish.blocks.page_actions')

    != self.partial('@market.blocks.search_form_wish')

    .market-search-wish__results
      .market-search-wish__loading-next.nojs-hide
        span.icon.icon-loading-placeholder
